
<form class="" method="POST" action="cadastrar">
  <div class="form-row">
    
    <div class="form-group col-md-6">
      <label for="inpEmail">E-mail</label>
      <input type="email" class="form-control" id="inpEmail" name="inpEmail" placeholder="E-mail">
    </div>
  </div>
  <div class="form-group">
    <label for="inpNome">Nome completo</label><ion-icon name="star-outline" hidden></ion-icon>
    <input type="text" class="form-control" id="inpNome" name="inpNome" placeholder="">

    <label for="inpFacul">Marcar?</label>
      <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="radioMarcar" id="Radio1" value="1">
          <label class="form-check-label" for="inlineRadio1">Sim</label>
        </div>
        <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="radioMarcar" id="Radio2" value="0">
          <label class="form-check-label" for="inlineRadio2">Não</label>
      </div>
  </div>
  <div class="form-group">
    <label for="inpTelefone">Telefone</label>
    <input type="text" class="form-control" id="inpTelefone" name="inpTelefone" placeholder="(24) 3333-4444">
  </div>
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="inpData">Data de nascimento</label>
      <input type="date" class="form-control" name="inpNascimento" id="inpNascimento">
    </div>
    <div class="form-group col-md-4">
      <label for="inpFacul">Cursando faculdade?</label>
      <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="radioFacul" id="inlineRadio1" value="1">
          <label class="form-check-label" for="inlineRadio1">Sim</label>
        </div>
        <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="radioFacul" id="inlineRadio2" value="0">
          <label class="form-check-label" for="inlineRadio2">Não</label>
      </div>
    </div>
    <div class="form-group col-md-2">
      <label for="inpSalarial">Pretenção salarial</label>
      <input type="text" class="form-control" name="inpSalarial" id="inpSalarial">
    </div>
  </div>
  <div class="form-row">
      <label for="inpHabilidades">Habilidades e competências</label>
      <textarea name="inpHabilidades" id="inpHabilidades" name="inpHabilidades" cols="30" rows="10" class="form-control"></textarea>
  </div>

  <button type="submit" class="btn btn-primary" name="btnCadastrar">Cadastrar</button>
</form>
